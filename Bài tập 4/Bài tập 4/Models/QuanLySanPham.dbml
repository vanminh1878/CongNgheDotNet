      <Column Name="CatalogCode" Type="System.String" DbType="NVarChar(250) NOT NULL" CanBeNull="false" />
      <Column Name="CatalogName" Type="System.String" DbType="NVarChar(250) NOT NULL" CanBeNull="false" />
